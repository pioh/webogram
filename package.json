  "scripts": {
    "gh-pages": "rm -rf docs && yarn prod && \\cp -r ./dist/* ./ && mv dist docs",
    "prod": "rm -rf dist && NODE_ENV=production yarn webpack",
    "fix": "yarn prettier \"./**/*.{ts,js,json,scss,css,jsx,tsx,html}\" --write && node ./node_modules/.bin/tslint --parallel 24 --project . --fix",
    "fixp": "find src/api/proto/ -name '*.ts' | parallel node_modules/.bin/tslint -p . --fix",
    "start": "yarn webpack --watch"
  },
